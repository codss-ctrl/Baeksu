{% extends 'layout_admin.html' %}
<meta charset="UTF-8">
{% block content %}

<link rel="stylesheet" href="css/common.css">
<script type="text/javascript">
	function fn_list() {
		location.href = "notice_list_admin"
	}
	function fn_mod() {
		document.frm.submit();
	}
</script>

<form name="frm" action="notice_addact" method="post" enctype="multipart/form-data">
	<form id="question_send">
		<table id="notice_mod" border="1">
			<tr>
				<th class="memo_title">제목</th>
				<td><input class="notice_text_box" type="text" name="notice_title" placeholder="제목입력" /></td>
			</tr>
			<tr>
				<th>내용</th>
				<td><textarea name="notice_content" rows="15" cols="80" placeholder="새소식에 올릴 내용을 입력해주세요."></textarea></td>
			</tr>
			<tr>
				<th colspan="2">
					<input type="button" class="notice_set_button" value="작성완료" onclick="fn_mod()"> <input type="button" class="notice_set_button" value="돌아가기" onclick="fn_list()">
				</th>
			</tr>
		</table>
	</form>
</form>

{% endblock %}